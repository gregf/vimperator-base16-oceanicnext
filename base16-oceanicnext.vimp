hi clear

hi Bell               border: none; background-color: #d3d0c8;
hi Boolean            color: #66cccc;
hi CmdLine            background: #34D46; color: #d3d0c8; transition: all 0.25s;
hi CompDesc           width: 500px; max-width: 500px; color: #66cccc;
hi CompItem[selected] background: #FAC863; color: #343D46; font-weight: bold;
hi CompMsg            font-style: italic; margin-left: 16px;
hi CompTitle          font-weight: bold;
hi CompTitle>*        color: #e8e6df; padding: 1px 0.5ex; border-bottom: 1px solid #d3d0c8;
hi ContentSeparator   display: -moz-box;
hi Disabled           color: #f2777a;
hi Enabled            color: #99cc99; font-weight: bold;
hi ErrorMsg           color: #e8e6df; background: #343D46; font-weight: bold;
hi Filter             color: #e8e6df; background: #cc99cc;
hi FrameIndicator     background-color: #1B2B34; opacity: 0.5; z-index: 999; position: fixed; top: 0; bottom: 0; left: 0; right: 0;
hi Function           color: #6699cc;
hi Help               color: #CDD3DE; background: #343D46;
hi HelpArg            color: #6699cc;
hi HelpBorder         border-color: #d3d0c8;border-width: 0px; border-style: solid;
hi HelpEx             display: inline-block; color: #6699cc; font-weight: bold;
hi HelpHead           display: block; margin: 1em 0; padding-bottom: .2ex; border-bottom-width: 1px; font-size: 2em; font-weight: bold; color: #6699cc; clear: both;
hi HelpInfoLabel      display: inline-block; width: 6em;  color: #cc99cc; font-weight: bold; vertical-align: text-top;
hi HelpKey            color: #e8e6df;
hi HelpLink           color: #66cccc; text-decoration: none;
hi HelpNote           color: #f2777a; font-weight: bold;
hi HelpOpt            color: #99cc99;
hi HelpOptionalArg    color: #6699cc;
hi HelpSpec           display: block; margin-left: -10em; float: left; clear: left; color: #cc99cc;
hi HelpString         display: inline-block; color: #99cc99; font-weight: normal; vertical-align: text-top;
hi HelpSubhead        display: block; margin: 1em 0; padding-bottom: .2ex; border-bottom-width: 1px; font-size: 1.2em; font-weight: bold; color: #6699cc; clear: both;
hi HelpSubsubhead     display: block; margin: 1em 0; padding-bottom: .2ex; font-size: 1.1em; font-weight: bold; color: #6699cc; clear: both;
hi HelpTag            display: inline-block; color: #515151; margin-left: 1ex; font-size: 0.65em; font-weight: bold;
hi HelpTopic          color: #e8e6df;
hi HelpWarning        color: #f2777a; font-weight: bold;
hi Hint               font-size: 12px; min-width: 2em; text-align: center; text-transform: uppercase; color: #e8e6df; background-color: #f2777a; box-shadow: 0px 2px 5px 0px rgba(0, 0, 0, 0.26); opacity: 0.9; z-index: 9999;
hi Hint::after        content: attr(number);
hi HintActive         color: #CDD3DE; text-shadow: none; background-color: #1B2B34; box-shadow: 0px 2px 5px 0px rgba(0, 0, 0, 0.26);
hi HintElem           color: #e8e6df; text-shadow: none; background-color: #4F5B66;
hi Indicator          color: #CDD3DE;
hi InfoMsg            color: #CDD3DE; background: #45F5B66;
hi Keyword            color: #f2777a;
hi LineNr             color: #CDD3DE; background: #1B2B34;
hi Mapping            color: #cc99cc;
hi ModeMsg            color: #343D46; background: #99C794; border-radius:2px; padding: 0px 5px; font-weight: bold;
hi MoreMsg            color: #CDD3DE; background: #2d2d2d;
hi NonText            color: #6699cc; min-height: 16px; padding-left: 2px;
